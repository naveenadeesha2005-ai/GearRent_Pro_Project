<VBox spacing="10" xmlns:fx="http://javafx.com/fxml"
      fx:controller="main.java.com.home.gearrentpro.controller.BranchController">

    <Label text="Branch Management" style="-fx-font-size:16;"/>

    <TextField fx:id="txtCode" promptText="Branch Code"/>
    <TextField fx:id="txtName" promptText="Branch Name"/>
    <TextField fx:id="txtAddress" promptText="Address"/>
    <TextField fx:id="txtContact" promptText="Contact No"/>

    <HBox spacing="10">
        <Button text="Save"/>
        <Button text="Update"/>
        <Button text="Delete"/>
    </HBox>

    <TableView fx:id="tblBranches">
        <columns>
            <TableColumn text="Code"/>
            <TableColumn text="Name"/>
            <TableColumn text="Contact"/>
        </columns>
    </TableView>

</VBox>
